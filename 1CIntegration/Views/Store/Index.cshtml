    <script>
        var html5Slider = document.getElementById('html5');

        noUiSlider.create(html5Slider, {
            start: [1500, 5000],
            connect: true,
            range: {
                'min': 0,
                'max': 10000
            }
        });
    </script>
    <script>
        var inputNumber2 = document.getElementById('input-number2');
        var inputNumber1 = document.getElementById('input-number1');
        
        html5Slider.noUiSlider.on('update', function (values, handle) {

            var value = values[handle];

            if (handle) {
                inputNumber2.value = Math.round(value);
            } else {
                inputNumber1.value = Math.round(value);
            }
        });

        inputNumber2.addEventListener('change', function () {
            html5Slider.noUiSlider.set([null, this.value]);
        });
        inputNumber1.addEventListener('change', function () {
            html5Slider.noUiSlider.set([this.value, null]);
        });
    </script>
    <script>
        window.onscroll = Djinaro.showVisible;
        Djinaro.filterByGoods();
        Djinaro.showVisible();

        jQuery(function () {
            var cache = {},
                lastXhr,
                list_search = [];
            var search = jQuery("#searchinput").autocomplete({
                minLength: 1,
                maxHeight: 400, // Максимальная высота списка подсказок, в пикселях
                width: 300, // Ширина списка
                zIndex: 9999, // z-index списка
                deferRequestBy: 0,
                source: function (request, response) {
                    var term = request.term;
                    if (term in cache) {
                        response(cache[term]);
                        return;
                    }
                    lastXhr = jQuery.getJSON("store/search", request, function (data, status, xhr) {
                        cache[term] = data;
                        if (xhr === lastXhr) {
                            response(data);
                        }
                    });
                },
                select: function (event, ui) {
                    var label = ui.item.label;
                    var value = ui.item.value;
                    //store in session
                    document.valueSelectedForAutocomplete = list_search.toString() + value
                }
            });
        });

