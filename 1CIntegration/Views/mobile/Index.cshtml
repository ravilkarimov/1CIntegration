    <script>
        jQuery(function () {
            Djinaro.getAllBrands();
            Djinaro.getAllSizes();
            Djinaro.filterMobileByGoods();
            var timeNoUi = setTimeout(function () { }, 500);
            jQuery("#search-terms")
                .keyup(function() {
                    clearInterval(timeNoUi);
                    timeNoUi = setTimeout(Djinaro.filterMobileByGoods(), 300);
                })
                .keyup();

            var height = Djinaro.ByClassName("fixed-header")[0].offsetHeight;
            Djinaro.ById("goods").style.marginTop = height + 'px';

            jQuery(document).ready(function () {
                jQuery(window).keydown(function (event) {
                    if (event.keyCode == 13) {
                        event.preventDefault();
                        return false;
                    }
                });
            });
        });

        var html5Slider = Djinaro.ById('html5');
        noUiSlider.create(html5Slider, {
            start: [1500, 5000],
            connect: true,
            range: {
                'min': 0,
                'max': 10000
            }
        });

        var inputNumber2 = Djinaro.ById('input-number2');
        var inputNumber1 = Djinaro.ById('input-number1');

        html5Slider.noUiSlider.on('update', function (values, handle) {

            var value = values[handle];
            if (handle) {
                inputNumber2.value = Math.round(value);
            } else {
                inputNumber1.value = Math.round(value);
            }
        });

        inputNumber2.addEventListener('change', function () {
            html5Slider.noUiSlider.set([null, this.value]);
        });
        inputNumber1.addEventListener('change', function () {
            html5Slider.noUiSlider.set([this.value, null]);
        });
    </script>



